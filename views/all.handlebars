 <div id="wrapper">
     {{#if article}}
      <h2>All Articles</h2>
      {{#each article}}
      <div style="border: 1px solid black; margin: 10px; padding: 10px;">
      <div id="articles">
        <h2>{{this.title}}</h2>
        <p><a href="{{this.link}}">{{this.link}}</a></p>

      </div>
    
      <div id="notes">
          <form action="/saveArticle" method="POST">
          
          <input type="hidden" name="title" value="{{this.title}}">
          <input type="hidden" name="link" value="{{this.link}}">
          <button>Save Article</button>
          </form>
      </div>
      </div>
      {{/each}}
      {{/if}}
    </div>